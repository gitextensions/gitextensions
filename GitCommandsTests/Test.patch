+                    {
+                        //WTF! No change
+                        if (input.StartsWith("diff --git "))
+                        {
+                            //No change? lets continue to the next line
+                            continue;
+                        }
+
+                        //new file!
+                        if (input.StartsWith("new file mode "))
+                            patch.Type = Patch.PatchType.NewFile;
+                        else if (input.StartsWith("deleted file mode "))
