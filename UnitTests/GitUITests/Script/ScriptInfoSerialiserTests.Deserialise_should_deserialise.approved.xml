<ArrayOfScriptInfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <ScriptInfo>
    <Enabled>false</Enabled>
    <Name>Fetch changes after commit</Name>
    <Command>git</Command>
    <Arguments>fetch</Arguments>
    <AddToRevisionGridContextMenu>false</AddToRevisionGridContextMenu>
    <OnEvent>AfterCommit</OnEvent>
    <AskConfirmation>true</AskConfirmation>
    <RunInBackground>false</RunInBackground>
    <IsPowerShell>false</IsPowerShell>
    <HotkeyCommandIdentifier>9000</HotkeyCommandIdentifier>
    <Icon>bug</Icon>
  </ScriptInfo>
  <ScriptInfo>
    <Enabled>false</Enabled>
    <Name>Update submodules after pull</Name>
    <Command>git</Command>
    <Arguments>submodule update --init --recursive</Arguments>
    <AddToRevisionGridContextMenu>false</AddToRevisionGridContextMenu>
    <OnEvent>AfterPull</OnEvent>
    <AskConfirmation>true</AskConfirmation>
    <RunInBackground>false</RunInBackground>
    <IsPowerShell>false</IsPowerShell>
    <HotkeyCommandIdentifier>9001</HotkeyCommandIdentifier>
    <Icon>bug</Icon>
  </ScriptInfo>
  <ScriptInfo>
    <Enabled>false</Enabled>
    <Name>Example</Name>
    <Command>c:\windows\system32\calc.exe</Command>
    <Arguments />
    <AddToRevisionGridContextMenu>false</AddToRevisionGridContextMenu>
    <OnEvent>ShowInUserMenuBar</OnEvent>
    <AskConfirmation>false</AskConfirmation>
    <RunInBackground>false</RunInBackground>
    <IsPowerShell>false</IsPowerShell>
    <HotkeyCommandIdentifier>9002</HotkeyCommandIdentifier>
    <Icon>bug</Icon>
  </ScriptInfo>
  <ScriptInfo>
    <Enabled>false</Enabled>
    <Name>Open on GitHub</Name>
    <Command>{openurl}</Command>
    <Arguments>https://github.com{cDefaultRemotePathFromUrl}/commit/{sHash}</Arguments>
    <AddToRevisionGridContextMenu>true</AddToRevisionGridContextMenu>
    <OnEvent>None</OnEvent>
    <AskConfirmation>false</AskConfirmation>
    <RunInBackground>false</RunInBackground>
    <IsPowerShell>false</IsPowerShell>
    <HotkeyCommandIdentifier>9003</HotkeyCommandIdentifier>
    <Icon>bug</Icon>
  </ScriptInfo>
  <ScriptInfo>
    <Enabled>false</Enabled>
    <Name>Fetch All Submodules</Name>
    <Command>git</Command>
    <Arguments>submodule foreach --recursive git fetch --all</Arguments>
    <AddToRevisionGridContextMenu>true</AddToRevisionGridContextMenu>
    <OnEvent>None</OnEvent>
    <AskConfirmation>false</AskConfirmation>
    <RunInBackground>false</RunInBackground>
    <IsPowerShell>false</IsPowerShell>
    <HotkeyCommandIdentifier>9004</HotkeyCommandIdentifier>
    <Icon>bug</Icon>
  </ScriptInfo>
  <ScriptInfo>
    <Enabled>true</Enabled>
    <Name>test</Name>
    <Command>notepad</Command>
    <Arguments />
    <AddToRevisionGridContextMenu>false</AddToRevisionGridContextMenu>
    <OnEvent>None</OnEvent>
    <AskConfirmation>false</AskConfirmation>
    <RunInBackground>false</RunInBackground>
    <IsPowerShell>false</IsPowerShell>
    <HotkeyCommandIdentifier>9005</HotkeyCommandIdentifier>
    <Icon>bug</Icon>
  </ScriptInfo>
</ArrayOfScriptInfo>